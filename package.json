{
  "name": "vscode-sie",
  "displayName": "SIE Extension",
  "description": "SIE extension...",
  "version": "0.0.1",
  "publisher": "Simon Segerblom Rex",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Bookkeeping"
  ],
  "contributes": {
    "languages": [
      {
        "id": "sie",
        "extensions": [
          ".si",
          ".se",
          "sie"
        ],
        "aliases": [
          "SIE",
          "sie"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sie",
        "scopeName": "source.sie",
        "path": "./syntaxes/abc.tmGrammar.json"
      }
    ],
    "configurationDefaults": {
      "[sie]": {
        "files.autoGuessEncoding": false,
        "files.encoding": "cp437"
      }
    }
  }
}