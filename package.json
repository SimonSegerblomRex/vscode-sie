{
  "name": "vscode-sie",
  "displayName": "SIE",
  "description": "SIE (1-4) syntax highlighting",
  "version": "0.1.0",
  "publisher": "SimonSegerblomRex",
  "readme": "README.md",
  "author": {
    "name": "Simon Segerblom Rex"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "engines": {
    "vscode": "^1.28.0"
  },
  "bugs": {
    "url": "https://github.com/SimonSegerblomRex/vscode-sie/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SimonSegerblomRex/vscode-sie"
  },
  "homepage": "https://github.com/SimonSegerblomRex/vscode-sie",
  "qna": "https://github.com/SimonSegerblomRex/vscode-sie/issues",
  "keywords": [
    "SIE",
    "accounting",
    "bookeeping"
  ],
  "categories": [
    "Programming Languages"
  ],
  "icon": "images/icon.png",
  "contributes": {
    "languages": [
      {
        "id": "sie",
        "extensions": [
          ".si",
          ".se",
          "sie"
        ],
        "aliases": [
          "SIE",
          "sie"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sie",
        "scopeName": "source.sie",
        "path": "./syntaxes/abc.tmGrammar.json"
      }
    ],
    "configurationDefaults": {
      "[sie]": {
        "files.autoGuessEncoding": false,
        "files.encoding": "cp437"
      }
    }
  }
}